{
    "version": 3,
    "builds": [
      { "src": "server/src/trpc/index.ts", "use": "@vercel/node" },
      { "src": "client/package.json", "use": "@vercel/static-build", "config": { "distDir": "client/build" } }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server/src/trpc/index.ts" },
      { "src": "/(.*)", "dest": "/client/index.html" }
    ]
}
  